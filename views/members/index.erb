<h1>Members</h1>

<table>
  <tr>
    <th>First Name</th>
    <th></th>
    <th>Last Name</th>
    <th></th>
    <th>E-mail</th>
  </tr>
  <% for member in @members %>
  <tr>
    <td><a href="/members/<%= member.id %>"><%= member.first_name %></a></td>
    <td>|</td>
    <td><%= member.email_address %></td>
  </tr>
  <% end %>
</table>
